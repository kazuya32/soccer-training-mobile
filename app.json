{
  "expo": {
    "name": "Soccer Hacker",
    "description": "Please learn great soccer skills.",
    "slug": "soccer-training-mobile",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.6.0",
    "orientation": "default",
    "primaryColor": "#1C86A9",
    "icon": "./assets/SoccerHackerIcon144-ios.png",
    "splash": {
      "image": "./assets/SoccerHackerSplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lifting.ios",
      "infoPlist": {
        "CFBundleDisplayName": "Soccer Hacker"
      }
    },
    "android": {
      "package": "com.lifting.android",
      "versionCode": 27,
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/SoccerHackerIcon144-android.png",
      "permissions": [
        "NOTIFICATIONS",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
  }
}
