{
  "expo": {
    "name": "Soccer Hacker",
    "description": "Please learn great soccer skills.",
    "slug": "soccer-training-mobile",
    "privacy": "public",
    "sdkVersion": "28.0.0",
    "platforms": ["ios", "android"],
    "version": "1.4.8",
    "orientation": "default",
    "primaryColor": "#1C86A9",
    "icon": "./assets/SoccerHackerIcon144-ios.png",
    "splash": {
      "image": "./assets/SoccerHackerSplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lifting.ios"
    },
    "android": {
      "package": "com.lifting.android",
      "versionCode": 12,
      "icon": "./assets/SoccerHackerIcon144-android.png"
    },
  }
}
